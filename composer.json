{
    "name": "tobymaxham/phoenixii-oauth2",
    "description": "A Package to use the OAuth2 API from PhoenixII.",
    "keywords": [
        "TobyMaxham",
        "phoenix-oauth",
        "oauth"
    ],
    "homepage": "https://github.com/tobymaxham/phoenixii-oauth2",
    "license": "MIT",
    "authors": [
        {
            "name": "TobyMaxham",
            "email": "git@maxham.de",
            "homepage": "https://maxham.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "TobyMaxham\\PhoenixAuth\\": "src"
        }
    },
    "require": {
        "php": ">8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpunit/phpunit": "^8.2"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
